
<p id="notice"><%= notice %></p>

<h1>Customer Address Details</h1>

<table class="table table-success table-striped">
  <thead>
    <tr>
      <th>Pickup address</th>
      <th>Delivery address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @details.each do |detail| %>
      <tr>
        <td><%= detail.pickup_address %></td>
        <td><%= detail.delivery_address %></td>

      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'Next', agents_path, class:"btn btn-info"  %>
<%= link_to "...", root_path %>

<br>


