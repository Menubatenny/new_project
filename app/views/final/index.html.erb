
<p id="notice"><%= notice %></p>

<h1>Agents Details</h1>

<table class="table table-success table-striped">
  <thead>
    <tr>
      <th>Agent name</th>
      <th>Phone number</th>
      <th>Delivery status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agents.each do |agent| %>
      <tr>
        <td><%= agent.agent_name %></td>
        <td><%= agent.phone_number %></td>
        <td><%= agent.delivery_status %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

<br>
